<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.stx.entity.merchant.Goods" table="goods" lazy="false">
		<id name="id" column="id" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="name" column="name" type="java.lang.String"/>
		<property name="price" column="price" type="java.lang.Float"/>
		<property name="goodsDesc" column="goods_desc" type="java.lang.String"/>
		<property name="goodsPic" column="goods_pic" type="java.lang.String"/>
		<property name="categoryId" column="category_id" type="java.lang.Long"/>
		<property name="monthCount" column="month_count" type="java.lang.Integer"/>
		<property name="buzz" column="buzz" type="java.lang.Integer"/>
	</class>
</hibernate-mapping>
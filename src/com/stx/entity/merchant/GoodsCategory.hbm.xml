<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.stx.entity.merchant.GoodsCategory" table="goods_category" lazy="false">
		<id name="id" column="id" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="categoryName" column="category_name" type="java.lang.String"/>
		<property name="merchantId" column="merchant_id" type="java.lang.Long"/>
		<property name="sort" column="sort" type="java.lang.Integer"/>
	</class>
</hibernate-mapping>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.stx.entity.order.PriceInfo" table="order_price_info" lazy="false">
		<id name="id" column="id" type="java.lang.Long">
			<generator class="native" />
		</id>
		<property name="orderId" column="order_id" type="java.lang.Long"/>
		<property name="amount" column="amount" type="java.lang.Float"/>
		<property name="balance" column="balance" type="java.lang.Float"/>
		<property name="boxPrice" column="box_price" type="java.lang.Float"/>
		<property name="deliveryPrice" column="delivery_price" type="java.lang.Float"/>
		<property name="payPrice" column="pay_price" type="java.lang.Float"/>
	</class>
</hibernate-mapping>
